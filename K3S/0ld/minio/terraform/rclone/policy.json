{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:HeadBucket"],
      "Resource": ["arn:aws:s3:::rclone-data"]
    },
    {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": ["arn:aws:s3:::rclone-data", "arn:aws:s3:::rclone-data/*"]
    }
  ]
}
